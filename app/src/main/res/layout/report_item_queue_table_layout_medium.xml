<?xml version="1.0" encoding="utf-8"?>
<TableRow xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_marginTop="1dp"
    android:layout_marginRight="1dp"
    android:layout_marginLeft="1dp"
    >
    <TextView
        android:id="@+id/report_item_tablerow_tv_name"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:background="@color/all_white"
        android:gravity="center"
        android:paddingTop="6dp"
        android:paddingBottom="6dp"
        android:paddingLeft="4dp"
        android:paddingRight="4dp"
        android:maxEms="4"
        android:textColor="@color/report_table_content"
        android:textSize="14sp"
        android:text=""/>
    <TextView
        android:id="@+id/report_item_tablerow_tv_access"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:background="@color/all_white"
        android:gravity="center"
        android:layout_marginLeft="1dp"
        android:paddingTop="6dp"
        android:paddingBottom="6dp"
        android:textColor="@color/report_table_content"
        android:textSize="14sp"
        android:text="0"/>
    <TextView
        android:id="@+id/report_item_tablerow_tv_accept"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:background="@color/all_white"
        android:gravity="center"
        android:layout_marginLeft="1dp"
        android:paddingTop="6dp"
        android:paddingBottom="6dp"
        android:textColor="@color/report_table_content"
        android:textSize="14sp"
        android:text="0"/>
    <TextView
        android:id="@+id/report_item_tablerow_tv_progress"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:background="@color/all_white"
        android:gravity="center"
        android:layout_marginLeft="1dp"
        android:paddingTop="6dp"
        android:paddingBottom="6dp"
        android:textColor="@color/report_table_content"
        android:textSize="14sp"
        android:text="0%"/>
</TableRow>