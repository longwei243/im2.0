<?xml version="1.0" encoding="utf-8"?>
<TableRow xmlns:android="http://schemas.android.com/apk/res/android">
    <TextView
        android:id="@+id/report_item_tablerow_tv_time"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:background="@drawable/bg_report_table"
        android:gravity="center"
        android:paddingTop="6dp"
        android:paddingBottom="6dp"
        android:paddingLeft="4dp"
        android:paddingRight="4dp"
        android:textColor="@color/report_table_content"
        android:textSize="18sp"
        android:text=""/>
    <TextView
        android:id="@+id/report_item_tablerow_tv_src"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:background="@drawable/bg_report_table"
        android:gravity="center"
        android:paddingTop="6dp"
        android:paddingBottom="6dp"
        android:textColor="@color/report_table_content"
        android:textSize="18sp"
        android:text="0"/>
    <TextView
        android:id="@+id/report_item_tablerow_tv_count"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:background="@drawable/bg_report_table"
        android:gravity="center"
        android:paddingTop="6dp"
        android:paddingBottom="6dp"
        android:textColor="@color/report_table_content"
        android:textSize="18sp"
        android:text="0"/>
</TableRow>